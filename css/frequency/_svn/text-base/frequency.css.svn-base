/**
	sexy-combo 2.1.3 : http://code.google.com/p/sexy-combo/
	
	This is an example custom skin.
*/
div.frequency {
	border-top-width: 0px;
	padding-right: 0px;
	padding-left: 0px;
	border-left-width: 0px;
	border-bottom-width: 0px;
	padding-bottom: 0px;
	margin: 0px;
	/*width: 146px;*/
	padding-top: 0px;
	white-space: nowrap;
	height: 24px;
	border-right-width: 0px
    
}

div.frequency input {
	border-right: #3a3c6c 1px solid;
	padding-right: 3px;
	border-top: 3px solid #fcb03b;
	padding-left: 3px;
	/*background: url(sexy-input-bg.jpg) #fff repeat-x 0px 0px;*/
	left: 0px;
	padding-bottom: 0px;
	margin: 0px;
	font: 11px/ 19px tahoma, arial, helvetica, sans-serif;
	vertical-align: bottom;
	border-left: 3px solid #fcb03b;
	width: 110px;
	padding-top: 0px;
	border-bottom: 3px solid #fcb03b;
	top: 0px;
	height: 21px;
	border-top-left-radius:10px;
	border-bottom-left-radius:10px;
}

div.frequency div.icon {
	border-top:3px solid #fcb03b;
	border-right:3px solid #fcb03b;
	border-bottom:3px solid #fcb03b;
	background: #fcb03b url(arrow.jpg) no-repeat 0px 0px;
	left: 110px;	
	width: 17px;
	cursor: pointer;
	top: 0px;
	height: 21px;
	border-top-right-radius:10px;
	border-bottom-right-radius:10px;
}

div.frequency div.list-wrapper {
	border-right: 3px solid #fcb03b;
	padding-right: 0px;
	border-top: 3px solid #fcb03b;
	padding-left: 0px;
	left: 0px;
	padding-bottom: 0px;
	margin: 0px;
	border-left: 3px solid #fcb03b;
	width: 124px; 
	bottom: auto;
	padding-top: 0px;
	border-bottom: 3px solid #fcb03b;
	top: 24px;
	background-color: #fcb03b;
	margin-top: -3px;
}

div.frequency div.list-wrapper-up {
	border-right: 3px solid #fcb03b;
	border-top: 3px solid #fcb03b;
	border-left: 3px solid #fcb03b;
	bottom: 30px;
	border-bottom: 3px solid #fcb03b;
	top: auto;
	margin-bottom: -12px;
}

div.frequency ul {
	padding-right: 0px;
	padding-left: 0px;
	padding-bottom: 0px;
	margin: 0px;
	padding-top: 0px;
	list-style-type: none;
	height: 200px;
	text-align: left;
}

div.frequency li {
	padding-right: 0px;
	padding-left: 5px;
	padding-bottom: 0px;
	margin: 0px;
	font: 11px 'Trebuchet MS',tahoma, arial, helvetica, sans-serif;
	cursor: pointer;
	/*padding-top: 4px; */
	background-color: #FFECCF;
}

div.frequency li.active {
	background-color: #fad49f;
}

div.frequency input.empty {
	color: gray;
}